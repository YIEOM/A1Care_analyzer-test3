<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/resultlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/result_bg" >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:src="@drawable/main_top_bg" />
            
    <ImageView
        android:id="@+id/device" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="520px"
        android:layout_marginTop="12px" />
    
   	<TextView
        android:id="@+id/timeText" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:typeface="sans"
        android:textSize="24px"
        android:textColor="#444444"
        android:shadowColor="#7F000000"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="1.2"
        android:layout_marginLeft="578px"
        android:layout_marginTop="15px" />

   	<Button
        android:id="@+id/homeicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="726px"
        android:background="@drawable/home_selector" />
 		   	       	    	
   	<TextView 
	    android:id="@+id/hba1cPct"
	    android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="84px"
        android:layout_centerHorizontal="true"
        android:typeface="sans"
        android:textColor="#1F3E6F"
        android:textSize="85px"
        android:textStyle="bold" />
   	
   	<TextView
	    android:id="@+id/hba1cUnit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/hba1cPct"
        android:layout_alignBaseline="@id/hba1cPct"
        android:typeface="sans"
        android:textColor="#1F3E6F"
        android:textStyle="bold"
        android:layout_marginLeft="2px" />
	 
   	<RelativeLayout 
	    android:layout_width="180px"
	    android:layout_height="wrap_content"
        android:layout_marginLeft="200px"
        android:layout_marginTop="208px" >
	    
		<TextView
	        android:id="@+id/primaryTitle"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="0px"
	        android:layout_centerHorizontal="true"
	        android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#565656" />
		
		<TextView
		    android:id="@+id/rangeTitle"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="30px"
	        android:layout_centerHorizontal="true"
	        android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#565656" />
		
		<TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="60px"
	        android:layout_centerHorizontal="true"
	        android:text="Cartridge Lot"
            android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#565656" />
		
		<TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="90px"
	        android:layout_centerHorizontal="true"
	        android:text="Test Date"
            android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#565656" />
		
		<TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="120px"
	        android:layout_centerHorizontal="true"
            android:text="Patient ID"
            android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#565656" />
	    	    
	</RelativeLayout>
   	
	<RelativeLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
        android:layout_marginLeft="418px"
        android:layout_marginTop="208px" >
	    
		<TextView
	        android:id="@+id/primary"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10px"
	        android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#242424"
	        android:layout_marginTop="0px" />
		
		<TextView
		    android:id="@+id/range"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10px"
	        android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#242424"
	        android:layout_marginTop="30px" />
		
		<TextView
		    android:id="@+id/unit"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
            android:layout_toRightOf="@id/range"
	        android:layout_alignBaseline="@id/range"
	        android:typeface="sans"
	        android:textSize="12px"
	        android:textColor="#242424"
	        android:layout_marginLeft="5px" />
		
		<TextView
		    android:id="@+id/testdate1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10px"
            android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#242424"
	        android:layout_marginTop="90px" />
		
		<TextView
		    android:id="@+id/testdate2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
            android:layout_toRightOf="@id/testdate1"
	        android:layout_alignBaseline="@id/testdate1"
	        android:typeface="sans"
	        android:textSize="12px"
	        android:textColor="#242424"
	        android:layout_marginLeft="5px" />
		
		<TextView
		    android:id="@+id/ref"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10px"
            android:typeface="sans"
	        android:textSize="20px"
	        android:textColor="#242424"
	        android:layout_marginTop="60px" />
		
		<EditText
	        android:id="@+id/patientidtext"
	        android:layout_width="168px"
	        android:layout_height="30px"
	        android:layout_marginTop="120px"
	        android:paddingLeft="10px"
	        android:ems="10"
	        android:textSize="20px"
	        android:textColor="#242424"
        	android:maxLength="20"
	        android:singleLine="true" 
	        android:imeOptions="actionDone"
	        android:longClickable="false"
	        android:cursorVisible="false"
	        android:background="@drawable/result_pid" />
	    	    
	</RelativeLayout>
           
	<Button
        android:id="@+id/convertbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="536px"
        android:layout_marginTop="198px"
        android:background="@drawable/result_convert_selector" />
	
    <Button
        android:id="@+id/printbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Print"
        android:typeface="sans"
        android:textSize="20px"
        android:textColor="#ffffff"
        android:layout_marginLeft="230px"
        android:layout_marginTop="396px"
        android:background="@drawable/btn_selector1" />   

    <Button
        android:id="@+id/nextsamplebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next Sample"
        android:typeface="sans"
        android:textSize="20px"
        android:textColor="#ffffff"
        android:layout_marginLeft="413px"
        android:layout_marginTop="396px"
        android:background="@drawable/btn_selector1" />
    	    
</RelativeLayout>